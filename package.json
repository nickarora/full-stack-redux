{
  "name": "redux-full-stack",
  "version": "0.0.2",
  "private": true,
  "scripts": {
    "start": "node ./dist/server.bundle.js",
    "dev": "NODE_ENV=development node ./dist/server.bundle.js & NODE_ENV=development node ./server.webpack.js",
    "build": "npm run build-client && npm run build-server",
    "test": "npm run test-client",
    "postinstall": "npm run build",
    "build-client": "webpack --config webpack.prod.config.js --progress --colors",
    "build-server": "webpack --config webpack.server.config.js --progress --colors",
    "test-browser": "./node_modules/karma/bin/karma start",
    "test-client": "NODE_ENV=test ./node_modules/.bin/mocha ./test/client/*.js --reporter spec --compilers js:babel-register",
    "test-server": "NODE_ENV=test ./node_modules/.bin/mocha ./test/server/*.js --reporter spec --compilers js:babel-register"
  },
  "dependencies": {
    "babel-core": "^6.4.5",
    "babel-eslint": "^5.0.0",
    "babel-loader": "^6.2.1",
    "babel-polyfill": "^6.3.14",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babel-preset-stage-0": "^6.3.13",
    "body-parser": "~1.13.2",
    "crypto": "0.0.3",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "ejs": "^2.3.4",
    "eslint": "1.10.3",
    "eslint-config-airbnb": "^5.0.1",
    "eslint-loader": "^1.2.1",
    "eslint-plugin-react": "^3.16.1",
    "express": "~4.13.1",
    "history": "^2.0.0-rc2",
    "json-loader": "^0.5.4",
    "mongodb": "^2.1.4",
    "mongoose": "^4.3.7",
    "node-loader": "^0.5.0",
    "node-sass": "^3.4.2",
    "postcss-loader": "^0.8.0",
    "react": "^0.14.6",
    "react-dom": "^0.14.6",
    "react-redux": "^4.0.6",
    "react-router": "^2.0.0-rc5",
    "react-router-redux": "^3.0.0",
    "redux": "^3.0.5",
    "redux-logger": "^2.3.2",
    "redux-thunk": "^1.0.3",
    "sass-loader": "^3.1.2",
    "serve-static": "^1.10.2",
    "style-loader": "^0.13.0",
    "superagent": "^1.7.2",
    "webpack": "^1.12.11"
  },
  "devDependencies": {
    "babel-plugin-rewire": "1.0.0-beta-3",
    "babel-register": "^6.4.3",
    "chai": "^3.4.1",
    "expect": "^1.14.0",
    "karma": "^0.13.19",
    "karma-chai": "^0.1.0",
    "karma-chrome-launcher": "^0.2.2",
    "karma-mocha": "^0.2.1",
    "karma-mocha-reporter": "^1.1.5",
    "karma-phantomjs-launcher": "^0.2.3",
    "karma-sinon": "^1.0.4",
    "karma-webpack": "^1.7.0",
    "mocha": "^2.3.4",
    "nock": "^7.0.2",
    "phantomjs": "^1.9.19",
    "react-hot-loader": "^1.3.0",
    "redux-devtools": "^3.0.1",
    "redux-devtools-dock-monitor": "^1.0.1",
    "redux-devtools-log-monitor": "^1.0.2",
    "redux-mock-store": "0.0.6",
    "sinon": "^1.17.3",
    "webpack-dev-server": "^1.14.1"
  }
}
